{
  "name": "dockerized-cli",
  "version": "0.6.0",
  "author": "ItamarB @benzaita",
  "bin": {
    "dockerized": "./bin/run"
  },
  "bugs": "https://github.com/benzaita/dockerized-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.15",
    "@oclif/config": "^1.13.0",
    "@oclif/plugin-help": "^2.2.0",
    "debug": "^3.1.0",
    "dnif": "^0.2.0",
    "js-yaml": "^3.13.1",
    "md5": "^2.2.1",
    "ramda": "^0.25.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.3.3",
    "@oclif/dev-cli": "^1.22.0",
    "@types/debug": "^4.1.4",
    "@types/jest": "^24.0.15",
    "@types/js-yaml": "^3.12.1",
    "@types/md5": "^2.1.33",
    "@types/node": "^10.14.10",
    "@types/ramda": "^0.26.12",
    "@types/yargs": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-jest": "^24.8.0",
    "deltree": "^1.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.1.0",
    "globby": "^8.0.2",
    "husky": "^0.14.3",
    "jest": "^24.8.0",
    "lint-staged": "^7.2.2",
    "pkg": "^4.4.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "rootDir": "src"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/benzaita/dockerized-cli",
  "keywords": [
    "oclif"
  ],
  "lint-staged": {
    "*.js": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  },
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "dockerized",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "benzaita/dockerized-cli",
  "scripts": {
    "build:standalone": "npx pkg -t node10-osx-x64,node10-linux-x64 --out-path build/ .",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "format": "prettier --write 'src/**'",
    "lint": "eslint 'src/**'",
    "test": "jest",
    "precommit": "lint-staged"
  },
  "types": "lib/index.d.ts"
}
